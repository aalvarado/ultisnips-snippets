snippet desc "describe" !b
describe('${1}', () => {
	${2}
})${3}
endsnippet

snippet it "it" !b
it('${1}', () => {
	${2}
})${3}
endsnippet

snippet "<([\w\.]+)" "jsx tag" br
<`!p snip.rv = match.group(1)`${1}>
	${VISUAL}${2}
</`!p snip.rv = match.group(1)`>${3}
endsnippet

snippet ={ "equal key jsx tag" i
={${1}}${2}
endsnippet
